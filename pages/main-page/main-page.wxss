.head {
  /* line-height: 1; */
}

.special {
  display: flex;
  flex-direction: row;
  width: 100%;
  list-style: none;
  margin: 20rpx 0;
}
.special .navitem {
  flex: 1 1 150rpx;
  text-align: center;
  padding-top: 20rpx;
  border-right: 2rpx solid #f4f4f4;
}
.special .navitem:last-child {
  border-right: none;
}
.special .navitem .navimg {
  width: 75rpx;
  height: 75rpx;
}
.special .navitem .title {
  margin-top: 10rpx;
  display: block;
  font-size: 32rpx;
}

.search-icon {
  position: fixed;
  top: 18rpx;
  right: 20rpx;
  opacity: .8;  
}